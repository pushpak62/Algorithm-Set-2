#include <bits/stdc++.h>
using namespace std;

int main()
{
    int arr[]={1,1,3,4,5,9};
    int n= sizeof(arr)/sizeof(arr[0]);
    int target=7;
    int first=0,last=n,mid;
    while(first<=last){
        mid=arr[first]+arr[last];
        if(mid==target){
            cout<<first<<" "<<last;
            break;
            
        }
        else if (target>mid){
            first++;
        }
        else {
            last--;
        }
    }
    return 0;
}
