#include<bits/stdc++.h>
using namespace std;

int main(){
    int arr[]={0, 1, 0, 1, 0, 0, 1, 1, 1, 0};
    int n=sizeof(arr)/sizeof(arr[0]);
    
    int first=0,last=n-1;
    while(last>first){
        if(arr[first]==0){
            first++;
        } else if (arr[last]==1){
            last--;
        }
        else {
            swap(arr[first],arr[last]);
            first++;
            last--;
        }
    }
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
        
    }
}
    
