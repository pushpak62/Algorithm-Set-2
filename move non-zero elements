#include<bits/stdc++.h>
using namespace std;
int main(){
    int arr[]={8,5,0,4,7,0,5,9};
    int n = sizeof(arr)/sizeof(arr[0]);
    
    int NonZeroIndex=0;
    for(int i=0;i<n;i++){
        if(arr[i]!=0){
            swap(arr[i],arr[NonZeroIndex]);
            NonZeroIndex++;
        }
    }
    for(int i=0;i<n;i++){
        cout<<arr[i];
    }
    
}
