#include<iostream>
using namespace std;

int main(){
    int arr[]={5,7,7,5,8,10};
    int n=sizeof(arr)/sizeof(arr[0]);
    
    int target;
    cin>>target;
    
    int first=0,last=n-1;
    int mid;
    bool found = false;
    while(first<=last){
        mid=(first+last)/2;
        if(arr[mid]==target){
            cout<<mid;
            found = true;
            break;
        }
        else if(arr[mid]>target){
            last=mid-1;
        }
        else {
            first=mid+1;
        }
    }
    if(!found){
        cout<<"target not found";
    }
    return 0;
    
}
