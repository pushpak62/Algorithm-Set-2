#include<bits/stdc++.h>
using namespace std;

int main(){
    int arr[]={2,7,5,8};
    int n = sizeof(arr)/sizeof(arr[0]);
    int target=9;
    unordered_map<int, int>mp;
    for(int i=0;i<n;i++){
        int remain=target-arr[i];
        
        if(mp.find(remain) != mp.end()){
            cout<<mp[remain]<<" "<<i;
            return 0;
        }
        mp[arr[i]]=i;
    }
    cout<<"No pair";
    return 0;
}
